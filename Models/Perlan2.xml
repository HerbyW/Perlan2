    <!--FLIGHT CONTROL SURFACES-->
    <animation>
        <type>rotate</type>
        <object-name>aileron-l</object-name>
        <property>surface-positions/left-aileron-pos-norm</property>
        <factor>-24</factor>
        <axis>
            <x1-m>0.39646</x1-m>
            <y1-m>-4.24197</y1-m>
            <z1-m>0.94206</z1-m>
            <x2-m>0.39646</x2-m>
            <y2-m>-11.39497</y2-m>
            <z2-m>1.10944</z2-m>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>aileron-r</object-name>
        <property>surface-positions/right-aileron-pos-norm</property>
        <factor>24</factor>
        <axis>
            <x1-m>0.39646</x1-m>
            <y1-m>4.24197</y1-m>
            <z1-m>0.94206</z1-m>
            <x2-m>0.39646</x2-m>
            <y2-m>11.39497</y2-m>
            <z2-m>1.10944</z2-m>
        </axis>
    </animation>
    
    <!--airbrakes-->
    <animation>
        <type>translate</type>
        <object-name>speedbrake-l</object-name>
        
        <property>surface-positions/speedbrake-pos-norm</property>
        <factor>0.12</factor>
        <center>
            <x-m>-1.17459</x-m>
            <y-m>-3.3164</y-m>
            <z-m>0.92774</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>
    
    <animation>
        <type>translate</type>
        <object-name>speedbrake-r</object-name>
        
        <property>surface-positions/speedbrake-pos-norm</property>
        <factor>0.12</factor>
        <center>
            <x-m>-1.17459</x-m>
            <y-m>3.3164</y-m>
            <z-m>0.92774</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>
    
    
    <!--Rudder  -->
    <animation>
        <type>rotate</type>
        <object-name>rudder</object-name>
	<object-name>rudder-blue</object-name>
	<object-name>rudder-red</object-name>
	<object-name>rudder-star</object-name>
	<object-name>rudder-grey</object-name>
        <property>surface-positions/rudder-pos-norm</property>
        <factor>-6</factor>
        <axis>
            <x1-m>6.09244</x1-m>
            <y1-m>0.0</y1-m>
            <z1-m>2.14261</z1-m>
            <x2-m>5.73801</x2-m>
            <y2-m>0.0</y2-m>
            <z2-m>0.85505</z2-m>
        </axis>
    </animation>
    
    
    
    <animation>
        <type>rotate</type>
        <object-name>elevator-l</object-name>
        <property>surface-positions/elevator-pos-norm</property>
        <factor>-6</factor>
        <axis>
            <x1-m>5.745</x1-m>
            <y1-m>-0.03915</y1-m>
            <z1-m>0.988</z1-m>
            <x2-m>5.745</x2-m>
            <y2-m>-1.270</y2-m>
            <z2-m>0.988</z2-m>
        </axis>
    </animation>
    
    <animation>
        <type>rotate</type>
        <object-name>elevator-r</object-name>
        <property>surface-positions/elevator-pos-norm</property>
        <factor>6</factor>
        <axis>
            <x1-m>5.745</x1-m>
            <y1-m>0.03915</y1-m>
            <z1-m>0.988</z1-m>
            <x2-m>5.745</x2-m>
            <y2-m>1.270</y2-m>
            <z2-m>0.988</z2-m>
        </axis>
    </animation>
    
    <!--helping wheels -->
    <model>
        <path>Aircraft/Perlan2/Models/wheels.ac</path>
    </model>
    
    <animation>
        <type>select</type>
        <object-name>halterung</object-name>
        <object-name>reifen</object-name>
	<object-name>speichen</object-name>
	<object-name>halterung.001</object-name>
        <object-name>reifen.001</object-name>
	<object-name>speichen.001</object-name>
        <condition>
            <equals>
                <property>sim/multiplay/generic/float[7]</property>
                <value>1</value>
            </equals>
        </condition>
  </animation>
  
  <animation>
    <type>spin</type>
        <object-name>reifen</object-name>
	<object-name>speichen</object-name>
    <property>sim/multiplay/generic/float[5]</property>
    <factor> -20 </factor>
    <center>
      <x-m> -0.001112 </x-m>
      <y-m>   -7.9 </y-m>
      <z-m>  0.311 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y>  1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>spin</type>
        <object-name>reifen.001</object-name>
	<object-name>speichen.001</object-name>
    <property>sim/multiplay/generic/float[5]</property>
    <factor> -20 </factor>
    <center>
      <x-m> -0.001112 </x-m>
      <y-m>   7.9 </y-m>
      <z-m>  0.311 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y>  1 </y>
      <z>  0 </z>
    </axis>
  </animation>
    
    
