    <!--SMOKE for aerobatics-->
    
    <model>
        <path>Effects/Smoke/smoke-blue.xml</path>
        <offsets>
            <x-m>0.42</x-m>
            <y-m>12.73</y-m>
            <z-m>1.15</z-m>
        </offsets>
    </model>
    <model>
        <path>Effects/Smoke/smoke-white.xml</path>
        <offsets>
            <x-m>0.42</x-m>
            <y-m>-12.73</y-m>
            <z-m>1.15</z-m>
        </offsets>
    </model>
    
    <!--GEAR-->
    <!--Compression-->
  
  <animation>
        <type>translate</type>
        <object-name>wheelM</object-name>
	
        <property>gear/gear[0]/compression-norm</property>
        <factor>0.09</factor>
        <axis>
            <z>1</z>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>wheelR</object-name>
        <property>gear/gear[1]/compression-norm</property>
        <factor>0.05</factor>
        <axis>
            <z>1</z>
        </axis>
    </animation>
  
  <animation>
    <type>spin</type>
        <object-name>wheelM</object-name>
    <property>sim/multiplay/generic/float[5]</property>
    <factor> -30 </factor>
    <center>
      <x-m> -0.63991 </x-m>
      <y-m>   0.00000 </y-m>
      <z-m>  0.16591 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y>  1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>spin</type>
    <object-name>wheelR</object-name>
    <property>sim/multiplay/generic/float[6]</property>
    <factor> -45 </factor>
    <center>
      <x-m> 5.4705 </x-m>
      <y-m>   0.00000 </y-m>
      <z-m>  0.55482 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y>  1 </y>
      <z>  0 </z>
    </axis>
  </animation>
    
    <!--CANOPY-->
    
    <animation>
    <object-name>Group5.001</object-name>
    <object-name>Group15.001</object-name>
</animation>

<animation>
<type>noshadow</type>
    <object-name>Group5.001</object-name>
    <object-name>Group15.001</object-name>
</animation>

<effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <condition>
      <property>/sim/rendering/rembrandt/enabled</property>
    </condition>
    <object-name>Group5.001</object-name>
    <object-name>Group15.001</object-name>
  </effect>
  
  <animation>
    <object-name>Group5.001</object-name>
    <object-name>Group15.001</object-name>
</animation>
    
